{
  "name": "spatialos-check-components",
  "version": "1.0.0",
  "description": "Tool for checking component usage in a SpatialOS project.",
  "keywords": [
    "spatialos",
    "improbable"
  ],
  "homepage": "https://github.com/ndkrempel/spatialos-check-components#readme",
  "bugs": {
    "url": "https://github.com/ndkrempel/spatialos-check-components/issues",
    "email": "ndkrempel@gmail.com"
  },
  "license": "ISC",
  "files": [
    "src"
  ],
  "main": "./src/index.js",
  "bin": {
    "check-components": "./bin/check-components"
  },
  "repository": "ndkrempel/spatialos-check-components",
  "scripts": {
    "pretest": "jshint src && jscs src && eslint src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": ""
  },
  "jshintConfig": {
    "eqeqeq": true,
    "esversion": 6,
    "noarg": true,
    "nonbsp": true,
    "shadow": false,
    "singleGroups": true,
    "strict": "global",
    "undef": true,
    "unused": "vars",
    "varstmt": true,

    "boss": true,
    "elision": true,
    "eqnull": true,
    "expr": true,
    "loopfunc": true,
    "scripturl": true,

    "laxbreak": true,
    "sub": true,

    "node": true,
    "typed": true,

    "-W058": true
  },
  "jscsConfig": {
    "disallowEmptyBlocks": {"allExcept": ["comments"]},
    "disallowIdenticalDestructuringNames": true,
    "disallowKeywordsInComments": ["TODO", "FIXME"],
    "disallowKeywordsOnNewLine": ["else"],
    "disallowKeywords": ["with"],
    "disallowMixedSpacesAndTabs": true,
    "disallowMultipleLineBreaks": true,
    "disallowMultipleLineStrings": true,
    "disallowNewlineBeforeBlockStatements": true,
    "disallowOperatorBeforeLineBreak": true,
    "disallowParenthesesAroundArrowParam": true,
    "disallowQuotedKeysInObjects": true,
    "disallowSpaceAfterObjectKeys": true,
    "disallowSpaceAfterPrefixUnaryOperators": true,
    "disallowSpaceBeforeComma": {"allExcept": ["sparseArrays"]},
    "disallowSpaceBeforePostfixUnaryOperators": true,
    "disallowSpaceBeforeSemicolon": true,
    "disallowSpacesInCallExpression": true,
    "disallowSpacesInFunction": {"beforeOpeningRoundBrace": true},
    "disallowSpacesInGenerator": {"afterStar": true},
    "disallowSpacesInsideBrackets": true,
    "disallowSpacesInsideParentheses": true,
    "disallowSpacesInsideParenthesizedExpression": true,
    "disallowSpacesInsideTemplateStringPlaceholders": true,
    "disallowTabs": true,
    "disallowTrailingWhitespace": true,
    "disallowUnusedVariables": true,
    "disallowVar": true,
    "disallowYodaConditions": true,
    "jsDoc": {
      "checkAnnotations": "closurecompiler",
      "checkParamExistence": true,
      "checkParamNames": true,
      "requireParamTypes": true,
      "checkRedundantParams": true,
      "checkReturnTypes": true,
      "checkRedundantReturns": true,
      "requireReturnTypes": true,
      "checkTypes": "strictNativeCase",
      "checkRedundantAccess": true,
      "leadingUnderscoreAccess": true,
      "disallowNewlineAfterDescription": true,
      "requireDescriptionCompleteSentence": true
    },
    "maximumLineLength": {"value": 80, "tabSize": 2, "allExcept": ["regex", "urlComments"]},
    "requireArrayDestructuring": true,
    "requireCamelCaseOrUpperCaseIdentifiers": true,
    "requireCapitalizedComments": {"inlined": true},
    "requireCapitalizedConstructorsNew": {"allExcept": ["Array", "Boolean", "Date", "Error", "Function", "Number", "Object", "RegExp", "String", "Symbol", "EvalError", "RangeError", "ReferenceError", "SyntaxError", "TypeError", "URIError"]},
    "requireCapitalizedConstructors": true,
    "requireCommaBeforeLineBreak": true,
    "requireDollarBeforejQueryAssignment": true,
    "requireEarlyReturn": true,
    "requireEnhancedObjectLiterals": true,
    "requireLineFeedAtFileEnd": true,
    "requireMatchingFunctionName": true,
    "requireNewlineBeforeSingleStatementsInIf": true,
    "requireNumericLiterals": true,
    "requireObjectDestructuring": true,
    "requirePaddingNewLinesAfterUseStrict": true,
    "requirePaddingNewLinesBeforeExport": true,
    "requireSemicolons": true,
    "requireShorthandArrowFunctions": true,
    "requireSpaceAfterBinaryOperators": true,
    "requireSpaceAfterComma": true,
    "requireSpaceAfterKeywords": true,
    "requireSpaceAfterLineComment": true,
    "requireSpaceBeforeBinaryOperators": true,
    "requireSpaceBeforeBlockStatements": true,
    "requireSpaceBeforeDestructuredValues": true,
    "requireSpaceBeforeKeywords": {"allExcept": ["typeof"]},
    "requireSpaceBeforeObjectValues": true,
    "requireSpaceBetweenArguments": true,
    "requireSpacesInConditionalExpression": true,
    "requireSpacesInForStatement": true,
    "requireSpacesInFunction": {"beforeOpeningCurlyBrace": true},
    "requireSpacesInGenerator": {"beforeStar": true},
    "requireSpread": true,
    "requireTemplateStrings": {"allExcept": ["stringConcatenation"]},
    "requireTrailingComma": {"ignoreSingleLine": true},
    "requireUseStrict": true,
    "safeContextKeyword": "that",
    "validateIndentation": 2,
    "validateParameterSeparator": ", ",
    "validateQuoteMarks": {"escape": false, "mark": "'", "ignoreJSX": true}
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "script"
    },
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-cond-assign": 0,
      "no-console": 2,
      "no-constant-condition": [2, {"checkLoops": true}],
      "no-control-regex": 2,
      "no-debugger": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty-character-class": 2,
      "no-empty": [2, {"allowEmptyCatch": true}],
      "no-ex-assign": 0,
      "no-extra-boolean-cast": 2,
      "no-extra-parens": [2, "all", {
        "conditionalAssign": true,
        "returnAssign": true,
        "nestedBinaryExpressions": true
      }],
      "no-extra-semi": 2,
      "no-func-assign": 2,
      "no-inner-declarations": 0,
      "no-invalid-regexp": 2,
      "no-irregular-whitespace": [2, {
        "skipStrings": true,
        "skipComments": false,
        "skipRegExps": false,
        "skipTemplates": true
      }],
      "no-obj-calls": 2,
      "no-prototype-builtins": 0,
      "no-regex-spaces": 0,
      "no-sparse-arrays": 0,
      "no-template-curly-in-string": 1,
      "no-unexpected-multiline": 2,
      "no-unreachable": 2,
      "no-unsafe-finally": 0,
      "no-unsafe-negation": 1,
      "use-isnan": 2,
      "valid-jsdoc": 2,
      "valid-typeof": 1,

      "accessor-pairs": 0,
      "array-callback-return": 1,
      "block-scoped-var": 2,
      "class-methods-use-this": 0,
      "complexity": 0,
      "consistent-return": 0,
      "curly": 0,
      "default-case": 0,
      "dot-location": [2, "property"],
      "dot-notation": 0,
      "eqeqeq": [2, "always", {"null": "ignore"}],
      "guard-for-in": 0,
      "no-alert": 2,
      "no-caller": 2,
      "no-case-declarations": 2,
      "no-div-regex": 0,
      "no-else-return": 2,
      "no-empty-function": [2, {"allow": []}],
      "no-empty-pattern": 2,
      "no-eq-null": 0,
      "no-eval": [2, {"allowIndirect": false}],
      "no-extend-native": 0,
      "no-extra-bind": 2,
      "no-extra-label": 2,
      "no-fallthrough": [2, {"commentPattern": "Fall through."}],
      "no-floating-decimal": 2,
      "no-global-assign": [2, {"exceptions": []}],
      "no-implicit-coercion": [2, {
        "boolean": true,
        "number": true,
        "string": true,
        "allow": ["!!", "+"]
      }],
      "no-implicit-globals": 2,
      "no-implied-eval": 2,
      "no-invalid-this": 1,
      "no-iterator": 2,
      "no-labels": [2, {"allowLoop": true, "allowSwitch": true}],
      "no-lone-blocks": 2,
      "no-loop-func": 0,
      "no-magic-numbers": [2, {
        "ignore": [0, 1, -1],
        "ignoreArrayIndexes": false,
        "enforceConst": true,
        "detectObjects": false
      }],
      "no-multi-spaces": [2, {"exceptions": {
        "Property": true,
        "SwitchCase": true
      }}],
      "no-multi-str": 2,
      "no-new-func": 1,
      "no-new-wrappers": 1,
      "no-new": 0,
      "no-octal-escape": 2,
      "no-octal": 2,
      "no-param-reassign": 0,
      "no-proto": 2,
      "no-redeclare": 0,
      "no-restricted-properties": [2, {
        "property": "__proto__",
        "message": "Please use Object.getPrototypeOf or Object.setPrototypeOf instead."
      }, {
        "property": "__lookupGetter__",
        "message": "Please use Object.getOwnPropertyDescriptor instead."
      }, {
        "property": "__lookupSetter__",
        "message": "Please use Object.getOwnPropertyDescriptor instead."
      }, {
        "property": "__defineGetter__",
        "message": "Please use Object.defineProperty instead."
      }, {
        "property": "__defineSetter__",
        "message": "Please use Object.defineProperty instead."
      }, {
        "property": "__iterator__",
        "message": "Please use Symbol.iterator instead."
      }, {
        "property": "__noSuchMethod__",
        "message": "Please use Proxy instead."
      }, {
        "property": "__count__",
        "message": "Please use for..in or Object.keys instead."
      }, {
        "property": "__parent__"
      }, {
        "object": "",
        "property": "escape",
        "message": "Please use encodeURI or encodeURIComponent instead."
      }, {
        "object": "",
        "property": "unescape",
        "message": "Please use decodeURI or decodeURIComponent instead."
      }, {
        "property": "substr",
        "message": "Please use String#substring or String#slice instead."
      }, {
        "property": "anchor"
      }, {
        "property": "big"
      }, {
        "property": "blink"
      }, {
        "property": "bold"
      }, {
        "property": "fixed"
      }, {
        "property": "fontcolor"
      }, {
        "property": "fontsize"
      }, {
        "property": "italics"
      }, {
        "property": "link"
      }, {
        "property": "small"
      }, {
        "property": "strike"
      }, {
        "property": "sub"
      }, {
        "property": "sup"
      }, {
        "property": "getYear",
        "message": "Please use Date#getFullYear instead."
      }, {
        "property": "setYear",
        "message": "Please use Date#setFullYear instead."
      }, {
        "property": "toGMTString",
        "message": "Please use Date#toUTCString instead."
      }, {
        "object": "",
        "property": "uneval"
      }, {
        "object": "Object",
        "property": "getNotifier",
        "message": "Please use Proxy instead."
      }, {
        "object": "Object",
        "property": "observe",
        "message": "Please use Proxy instead."
      }, {
        "object": "Object",
        "property": "unobserve",
        "message": "Please use Proxy instead."
      }, {
        "object": "Array",
        "property": "observe",
        "message": "Please use Proxy instead."
      }, {
        "object": "Array",
        "property": "unobserve",
        "message": "Please use Proxy instead."
      }, {
        "property": "watch",
        "message": "Please use a getter or Proxy instead."
      }, {
        "property": "unwatch",
        "message": "Please use a getter or Proxy instead."
      }, {
        "property": "toSource"
      }, {
        "property": "eval"
      }, {
        "property": "isGenerator"
      }, {
        "object": "Function.prototype",
        "property": "arguments"
      }, {
        "object": "Function.prototype",
        "property": "caller"
      }, {
        "property": "arity",
        "message": "Please use Function#length instead."
      }, {
        "property": "displayName",
        "message": "Please use Function#name instead."
      }, {
        "object": "RegExp",
        "property": "input",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$_",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "lastMatch",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$&",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "lastParen",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$+",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "leftContext",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$`",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "multiline",
        "message": "Please use RegExp#multiline instead."
      }, {
        "object": "RegExp",
        "property": "$*",
        "message": "Please use RegExp#multiline instead."
      }, {
        "object": "RegExp",
        "property": "rightContext",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$'",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$1",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$2",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$3",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$4",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$5",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$6",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$7",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$8",
        "message": "Please use the match object instead."
      }, {
        "object": "RegExp",
        "property": "$9",
        "message": "Please use the match object instead."
      }, {
        "property": "toLocaleFormat",
        "message": "Please use Date#toLocaleDateString instead."
      }, {
        "object": "Proxy",
        "property": "create",
        "message": "Please use new Proxy instead."
      }, {
        "object": "Proxy",
        "property": "createFunction",
        "message": "Please use new Proxy instead."
      }, {
        "property": "quote"
      }],
      "no-return-assign": 0,
      "no-return-await": 2,
      "no-script-url": 0,
      "no-self-assign": [2, {"props": true}],
      "no-self-compare": 1,
      "no-sequences": 0,
      "no-throw-literal": 2,
      "no-unmodified-loop-condition": 0,
      "no-unused-expressions": [2, {
        "allowShortCircuit": true,
        "allowTernary": true
      }],
      "no-unused-labels": 2,
      "no-useless-call": 1,
      "no-useless-concat": 0,
      "no-useless-escape": 2,
      "no-useless-return": 2,
      "no-void": 0,
      "no-warning-comments": [1, {
        "terms": ["todo", "fixme"],
        "location": "start"
      }],
      "no-with": 2,
      "radix": 0,
      "require-await": 1,
      "vars-on-top": 0,
      "wrap-iife": 0,
      "yoda": [2, "never", {
        "exceptRange": false,
        "onlyEquality": false
      }],

      "strict": [2, "global"],

      "init-declarations": 0,
      "no-catch-shadow": 0,
      "no-delete-var": 2,
      "no-label-var": 0,
      "no-restricted-globals": [2, "escape", "unescape", "uneval"],
      "no-shadow-restricted-names": 0,
      "no-shadow": 0,
      "no-undef-init": 2,
      "no-undef": 2,
      "no-undefined": 0,
      "no-unused-vars": [2, {
        "vars": "all",
        "varsIgnorePattern": "^_$",
        "args": "none",
        "caughtErrors": "all",
        "caughtErrorsIgnorePattern": "^_$"
      }],
      "no-use-before-define": 0,

      "callback-return": 0,
      "global-require": 2,
      "handle-callback-err": [1, "^(err|error)$"],
      "no-mixed-requires": 0,
      "no-new-require": 0,
      "no-path-concat": 2,
      "no-process-env": 0,
      "no-process-exit": 0,
      "no-restricted-modules": 0,
      "no-sync": 0,

      "array-bracket-spacing": [2, "never", {
        "singleValue": false,
        "objectsInArrays": false,
        "arraysInArrays": false
      }],
      "block-spacing": [2, "always"],
      "brace-style": [2, "1tbs", {"allowSingleLine": true}],
      "camelcase": [2, {"properties": "always"}],
      "capitalized-comments": [2, "always", {"line": {
        "ignoreConsecutiveComments": true
      }, "block": {
        "ignoreInlineComments": false,
        "ignoreConsecutiveComments": false
      }}],
      "comma-dangle": [2, {
        "arrays": "only-multiline",
        "objects": "only-multiline",
        "imports": "only-multiline",
        "exports": "only-multiline",
        "functions": "never"
      }],
      "comma-spacing": [2, {"before": false, "after": true}],
      "comma-style": [2, "last"],
      "computed-property-spacing": [2, "never"],
      "consistent-this": [2, "that"],
      "eol-last": [2, "always"],
      "func-call-spacing": [2, "never"],
      "func-name-matching": [1, "always", {"includeCommonJSModuleExports": true}],
      "func-names": [1, "as-needed"],
      "func-style": [2, "declaration", {"allowArrowFunctions": false}],
      "id-blacklist": [2, "err", "cb", "foo"],
      "id-length": 0,
      "id-match": [2, "^([a-z]+([A-Z]{1,2}[a-z]+)*_?|([A-Z]{1,2}[a-z]+)+|[A-Z][A-Z_]*|_)$", {
        "properties": true,
        "onlyDeclarations": false
      }],
      "indent": [2, 2, {
        "SwitchCase": 1,
        "VariableDeclarator": 2,
        "outerIIFEBody": 1,
        "MemberExpression": 2,
        "FunctionDeclaration": {"parameters": 2, "body": 1},
        "FunctionExpression": {"parameters": 2, "body": 1},
        "CallExpression": {"arguments": 2}
      }],
      "jsx-quotes": [2, "prefer-double"],
      "key-spacing": [2, {
        "beforeColon": false,
        "afterColon": true,
        "mode": "strict",
        "align": "value"
      }],
      "keyword-spacing": [2, {"before": true, "after": true}],
      "line-comment-position": 0,
      "linebreak-style": 0,
      "lines-around-comment": 0,
      "lines-around-directive": [2, {"before": "always", "after": "always"}],
      "max-depth": 0,
      "max-len": [2, {
        "code": 80,
        "tabWidth": 2,
        "comments": 80,
        "ignoreComments": false,
        "ignoreTrailingComments": false,
        "ignoreUrls": true,
        "ignoreStrings": false,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }],
      "max-lines": 0,
      "max-nested-callbacks": 0,
      "max-params": 0,
      "max-statements-per-line": 0,
      "max-statements": 0,
      "multiline-ternary": 0,
      "new-cap": [2, {
        "newIsCap": true,
        "capIsNew": true,
        "capIsNewExceptions": ["EvalError", "RangeError", "ReferenceError", "SyntaxError", "TypeError", "URIError"],
        "properties": true
      }],
      "new-parens": 0,
      "newline-after-var": 0,
      "newline-before-return": 0,
      "newline-per-chained-call": 0,
      "no-array-constructor": 2,
      "no-bitwise": 0,
      "no-continue": 0,
      "no-inline-comments": 0,
      "no-lonely-if": 2,
      "no-mixed-operators": 0,
      "no-mixed-spaces-and-tabs": 2,
      "no-multiple-empty-lines": [2, {"max": 1, "maxEOF": 1, "maxBOF": 0}],
      "no-negated-condition": 0,
      "no-nested-ternary": 0,
      "no-new-object": 2,
      "no-plusplus": 0,
      "no-restricted-syntax": 0,
      "no-tabs": 2,
      "no-ternary": 0,
      "no-trailing-spaces": [2, {"skipBlankLines": false}],
      "no-underscore-dangle": 0,
      "no-unneeded-ternary": [2, {"defaultAssignment": false}],
      "no-whitespace-before-property": 2,
      "object-curly-newline": [2, {"ObjectExpression": {"multiline": true}, "ObjectPattern": "never"}],
      "object-curly-spacing": [2, "never"],
      "object-property-newline": [2, {"allowMultiplePropertiesPerLine": true}],
      "one-var-declaration-per-line": 0,
      "one-var": 0,
      "operator-assignment": [2, "always"],
      "operator-linebreak": [2, "before"],
      "padded-blocks": [2, "never"],
      "quote-props": [2, "as-needed", {
        "keywords": false,
        "unnecessary": true,
        "numbers": false
      }],
      "quotes": [2, "single", {
        "avoidEscape": false,
        "allowTemplateLiterals": true
      }],
      "require-jsdoc": 0,
      "semi-spacing": [2, {"before": false, "after": true}],
      "semi": [2, "always", {"omitLastInOneLineBlock": false}],
      "sort-keys": 0,
      "sort-vars": 0,
      "space-before-blocks": [2, "always"],
      "space-before-function-paren": [2, "never"],
      "space-in-parens": [2, "never"],
      "space-infix-ops": 2,
      "space-unary-ops": [2, {"words": true, "nonwords": false}],
      "spaced-comment": [2, "always", {"line": {
        "markers": [],
        "exceptions": []
      }, "block": {
        "markers": ["*"],
        "exceptions": [],
        "balanced": true
      }}],
      "unicode-bom": [2, "never"],
      "wrap-regex": 0,

      "arrow-body-style": [2, "as-needed", {"requireReturnForObjectLiteral": false}],
      "arrow-parens": [2, "as-needed", {"requireForBlockBody": false}],
      "arrow-spacing": [2, {"before": true, "after": true}],
      "constructor-super": 2,
      "generator-star-spacing": [2, "before"],
      "no-class-assign": 2,
      "no-confusing-arrow": 0,
      "no-const-assign": 2,
      "no-dupe-class-members": 2,
      "no-duplicate-imports": [2, {"includeExports": false}],
      "no-new-symbol": 2,
      "no-restricted-imports": 0,
      "no-this-before-super": 2,
      "no-useless-computed-key": 2,
      "no-useless-constructor": 2,
      "no-useless-rename": [2, {
        "ignoreImport": false,
        "ignoreExport": false,
        "ignoreDestructuring": false
      }],
      "no-var": 2,
      "object-shorthand": [2, "always"],
      "prefer-arrow-callback": [2, {
        "allowNamedFunctions": true,
        "allowUnboundThis": true
      }],
      "prefer-const": [2, {
        "destructuring": "all",
        "ignoreReadBeforeAssign": true
      }],
      "prefer-numeric-literals": 2,
      "prefer-rest-params": 2,
      "prefer-spread": 2,
      "prefer-template": 2,
      "require-yield": 1,
      "rest-spread-spacing": [2, "never"],
